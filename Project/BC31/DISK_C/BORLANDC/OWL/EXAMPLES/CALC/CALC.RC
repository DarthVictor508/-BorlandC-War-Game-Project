  CONTROL "6" 54, "BUTTON", WS_CHILD | WS_VISIBLE | WS_TABSTOP, 43, 49, 14, 12
  CONTROL "7" 55, "BUTTON", WS_CHILD | WS_VISIBLE | WS_TABSTOP, 7, 35, 14, 12
  CONTROL "8" 56, "BUTTON", WS_CHILD | WS_VISIBLE | WS_TABSTOP, 25, 35, 14, 12
