  "2", 50, ASCII
  "3", 51, ASCII
  "4", 52, ASCII
  "5", 53, ASCII
  "6", 54, ASCII
  "7", 55, ASCII
  "8", 56, ASCII
  "9", 57, ASCII
  "%", 37, ASCII
  "/", 47, ASCII
  "*", 42, ASCII
  "-", 45, ASCII
  "+", 43, ASCII
