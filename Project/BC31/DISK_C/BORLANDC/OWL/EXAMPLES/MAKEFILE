!include "makefile.inc"

APPS = popup.exe \
       trantest.exe \
       cursapp.exe \
       helloapp.exe \
       scribapp.exe \
       vdlgapp.exe \
       editapp.exe \
       mdiapp.exe \
       scrolapp.exe \
       bscrlapp.exe \
       cboxtest.exe \
       mditest.exe \
       sbartest.exe \
       lboxtest.exe \
       btntest.exe \
       stattest.exe \
       ewndtest.exe \
       edittest.exe \
       dialtest.exe \
       paltest.exe \
       lbxttest.exe \
       insttest.exe \
       cbxttest.exe \
       gboxtest.exe \
       dctltest.exe \
       cctltest.exe \
       notitest.exe \
       mlbxttst.exe \
       calldll.exe \
       usecdll.exe \
       usecdll2.exe

all: $(APPS)

turboc.cfg: makefile makefile.inc
	echo -I$(INCLUDE) >turboc.cfg

popup.exe: turboc.cfg popup.obj popup.res
     $(TLINK) $(LFLAGS) $(BCLIBDIR)\c0w$(MODEL) popup,popup,,@&&!
$(OWLLIBS) $(STDLIBS)
!,$(OWLROOT)\lib\owl.def
     $(RC) -30 -K popup.res popup.exe

trantest.exe: turboc.cfg trantest.obj trantest.res
     $(TLINK) $(LFLAGS) $(BCLIBDIR)\c0w$(MODEL) trantest,trantest,,@&&!
$(OWLLIBS) $(STDLIBS)
!,$(OWLROOT)\lib\owl.def
     $(RC) -30 -K trantest.res trantest.exe

